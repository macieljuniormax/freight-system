<%= form_with(model: @deadline) do |f| %>
  <div>
    <%= f.label :min_distance %>
    <%= f.number_field :min_distance %>
  </div>
  <div>
    <%= f.label :max_distance %>
    <%= f.number_field :max_distance %>
  </div>
  <div>
    <%= f.label :deadline_days %>
    <%= f.number_field :deadline_days %>
  </div>
  <div>
    <%= f.submit 'Finalizar Cadastro' %>
  </div>
<% end %>